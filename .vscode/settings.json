{
  "files.associations": {
    "**/inventory/*.yml": "ansible",
    "**/tasks/*.yml": "ansible"
  },
  "files.exclude": {
    "ansible/ansible-deps-cache": true
  },
  "python.analysis.exclude": [
    "ansible/ansible-deps-cache/**"
  ],
  "ansible.python.interpreterPath": "ansible/ansible-deps-cache/bin/python3"
}
